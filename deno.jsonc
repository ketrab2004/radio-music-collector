{
    "tasks": {
        "cache": "deno cache main.ts",
        "info": "deno info && deno info main.ts",

        "watch": "deno run --watch --allow-all main.ts",
        "run": "deno run --allow-read --allow-write --allow-sys=osRelease,uid --allow-env --allow-net main.ts"
    },
    "imports": {
        "cmd-ts": "npm:cmd-ts@^0.13",
        "chrono": "npm:chrono-node@^2.7.5",
        "date-fns": "npm:date-fns@^3.3.1",
        "@action/core": "npm:@actions/core@^1.10.1"
    }
}
